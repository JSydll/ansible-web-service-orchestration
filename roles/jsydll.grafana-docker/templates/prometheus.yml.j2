scrape_configs:
  - job_name: 'data-collector'
    scrape_interval: 10s
    static_configs:
      - targets: ['data-collector:9090']
 
{% for job in grafana_prometheus_extra_jobs %} 
  - job_name: '{{ job.name }}'
    scrape_interval: 10s
    static_configs:
      - targets: ['{{ job.target }}']
{% endfor %}
 
  - job_name: 'sys-metrics'
    scrape_interval: 10s
    static_configs:
      - targets: ['sys-metrics:9100']
 
  - job_name: 'docker-metrics'
    scrape_interval: 15s
    static_configs:
      - targets: ['docker-metrics:8080']
